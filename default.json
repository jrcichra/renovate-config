{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": ["config:recommended"],
  "labels": ["dependencies"],
  "dependencyDashboard": true,
  "automerge": false,
  "prConcurrentLimit": 1,
  "prHourlyLimit": 0,
  "packageRules": [
    {
      "groupName": "all",
      "groupSlug": "all",
      "matchPackagePatterns": [".*"]
    }
  ],
 "schedule": ["monthly"]
}
